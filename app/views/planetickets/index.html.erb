<% provide(:title, "飞机票订单") %>

<h1>飞机票订单</h1>


<div class="row">
  <div class="col-md-2 col-md-offset-1">
      <%= link_to "新增", new_planeticket_path, class: "btn btn-primary" %>
  </div>
</div>

<table id="planetickets" class="display" data-source="<%= planetickets_url(:format => "json") %>">
  <thead>
    <tr>
      <th>ID</th>
      <th>类别</th>
      <th>订单ID</th>
      <th>小团号</th>
      <th>出发日期</th>
      <th>行程</th>
      <th>联运</th>
      <th>出票日期</th>
      <th>出票地</th>
      <th>游客姓名</th>
      <th>证件号码</th>
      <th>有效期</th>
      <th>出生日期</th>
      <th>票面</th>
      <th>手续费</th>
      <th>航班</th>
      <th>备注</th>
      <th>报票人</th>
      <th>报票备注</th>
      <th>票况</th>      
      <th>具体状态</th>
      <th>操作</th>
    </tr>
  </thead>
  <tbody>
  </tbody>
</table>
