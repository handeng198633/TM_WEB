<% provide(:title, "旅游线路") %>
<h1>旅游线路</h1>

<div class="row">
  <div class="col-md-2 col-md-offset-1">
      <%= link_to "新增", new_linelist_path, class: "btn btn-primary" %>
  </div>
</div>

<table id="linelists" class="display" data-source="<%= linelists_url(:format => "json") %>">
  <thead>
    <tr>
      <th>ID</th>
      <th>编码</th>
      <th>天数</th>
      <th>线路名称</th>
      <th>行程</th>
      <th>主配图</th>
      <th>文档</th>
      <th>价格发布</th>
      <th>修改人</th>
      <th>销售中</th>
      <th>操作</th>
    </tr>
  </thead>
  <tbody>
  </tbody>
</table>
