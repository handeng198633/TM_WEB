<% provide(:title, "净利润表") %>

<h1>净利润表</h1>

<div class="row">
  <div class="col-md-2 col-md-offset-2">
      <%= link_to "新增", new_netprofit_path, class: "btn btn-primary" %>
  </div>
</div>

<table id="netprofits" class="display" data-source="<%= netprofits_url(:format => "json") %>">
  <thead>
    <tr>
      <th>月份</th>
      <th>收入合计</th>
      <th>成本合计</th>
      <th>营业利润</th>
      <th>应交税费</th>
      <th>净利润</th>
      <th>状态</th>
      <th>详情</th>
      <th>操作</th>
    </tr>
  </thead>
  <tbody>
  </tbody>
</table>
